{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular-client"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [
        "assets/js/jquery.min.js",
        "assets/js/nprogress.js",
        "assets/js/bootstrap.min.js",
        "assets/js/progressbar/bootstrap-progressbar.min.js",
        "assets/js/nicescroll/jquery.nicescroll.min.js",
        "assets/js/moment/moment.min.js",
        "assets/js/datepicker/daterangepicker.js",
        "assets/js/chartjs/chart.min.js",
        "assets/js/flot/jquery.flot.js",
        "assets/js/flot/jquery.flot.pie.js",
        "assets/js/flot/jquery.flot.orderBars.js",
        "assets/js/flot/jquery.flot.time.min.js",
        "assets/js/flot/date.js",
        "assets/js/flot/jquery.flot.spline.js",
        "assets/js/flot/jquery.flot.stack.js",
        "assets/js/flot/curvedLines.js",
        "assets/js/flot/jquery.flot.resize.js",
        "assets/js/icheck/icheck.min.js",
        "assets/js/editor/bootstrap-wysiwyg.js",
        "assets/js/editor/external/jquery.hotkeys.js",
        "assets/js/editor/external/google-code-prettify/prettify.js",
        "assets/js/maps/jquery-jvectormap-2.0.3.min.js",
        "assets/js/maps/gdp-data.js",
        "assets/js/maps/jquery-jvectormap-world-mill-en.js",
        "assets/js/maps/jquery-jvectormap-us-aea-en.js",
        "assets/js/pace/pace.min.js",
        "assets/js/skycons/skycons.min.js",
        "assets/js/select/select2.full.js",
        "assets/js/autocomplete/jquery.autocomplete.js",
        "assets/js/autocomplete/countries.js",
        "assets/js/custom.js",
        "assets/js/dashboard.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
